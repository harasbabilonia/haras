<p id="notice"><%= notice %></p>

<h1>Tipo Vendas</h1>

<table>
  <thead>
    <tr>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_vendas.each do |tipo_venda| %>
      <tr>
        <td><%= tipo_venda.tipo %></td>
        <td><%= link_to 'Show', tipo_venda %></td>
        <td><%= link_to 'Edit', edit_tipo_venda_path(tipo_venda) %></td>
        <td><%= link_to 'Destroy', tipo_venda, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Venda', new_tipo_venda_path %>
